    {
      "parameters": {
        "jsCode": "// Analyze risk based on sensor readings\nconst { vibration, temperature } = $input.first().json;\n\nlet riskLevel = 'LOW';\nlet riskScore = 0;\n\nif (vibration > 7.0) riskScore += 2;\nif (vibration > 8.5) riskScore += 3;\nif (temperature > 85) riskScore += 2;\nif (temperature > 95) riskScore += 3;\n\nif (riskScore >= 5) {\n  riskLevel = 'HIGH';\n} else if (riskScore >= 2) {\n  riskLevel = 'MEDIUM';\n}\n\nreturn {\n  json: {\n    ...$input.first().json,\n    riskLevel: riskLevel,\n    riskScore: riskScore\n  }\n};"
      },
